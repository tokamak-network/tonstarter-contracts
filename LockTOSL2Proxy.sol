        require(
            Address.isContract(newAddress),
            "CoinageFactorySLOT: Cannot set a proxy coinage factory to a non-contract address"
        );

        bytes32 slot = COINAGEFACTORY_SLOT;

        assembly {
            sstore(slot, newAddress)
        }
